{
  "plugins": ["prettier"],
  "env": {
	"node": true
  },
  "parserOptions": {
	"ecmaVersion": 2015
  },
  "rules": {
	"no-underscore-dangle": [
	  0
	],
	"no-mixed-requires": [
	  0
	],
	"new-cap": [
	  0
	],
	"no-console": 2,
	"no-debugger": 2,
	"no-path-concat": 0,
	"no-shadow": 1,
	"no-array-constructor": 2,
	"no-caller": 2,
	"no-eval": 2,
	"no-extend-native": 2,
	"no-extra-bind": 2,
	"no-with": 2,
	"no-loop-func": 2,
	"no-multi-str": 2,
	"no-new-func": 2,
	"no-new-object": 2,
	"no-return-assign": 2,
	"no-sequences": 2,
	"no-shadow-restricted-names": 2,
	"no-unused-expressions": 2,
	"no-unused-vars": 2,
	"no-use-before-define": 0,
	"no-new": 0,
	"dot-notation": [
	  2,
	  {
		"allowKeywords": true
	  }
	],
	"eqeqeq": 2,
	"new-parens": 2,
	"strict": [
	  2,
	  "global"
	],
	"yoda": 2,
	"valid-jsdoc": 2,
	"object-shorthand": 2,
	"no-var": 2,
	"prefer-const": 2,
	"prefer-arrow-callback": 2,
	"arrow-body-style": [2, "as-needed", { "requireReturnForObjectLiteral": true }],
	"prettier/prettier": [2, {"useTabs": true, "singleQuote": true}]
  }
}
