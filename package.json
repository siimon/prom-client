{
  "name": "prom-client",
  "version": "9.1.1",
  "description": "Client for prometheus",
  "main": "index.js",
  "files": [
    "lib/",
    "index.js",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "npm run lint && npm run compile-typescript && npm run test-unit",
    "lint": "node-version-gte-4 && eslint . || node-version-lt-4",
    "test-unit": "jest",
    "compile-typescript": "tsc index.d.ts --noImplicitAny"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:siimon/prom-client.git"
  },
  "keywords": [
    "Prometheus",
    "Metrics",
    "Client"
  ],
  "author": "Simon Nyberg",
  "license": "Apache-2.0",
  "homepage": "https://github.com/siimon/prom-client",
  "devDependencies": {
    "eslint": "^3.5.0",
    "express": "^4.13.3",
    "jest": "^20.0.4",
    "nock": "^9.0.11",
    "node-version-check": "^2.1.1",
    "sinon": "^2.0.0",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "tdigest": "^0.1.1",
    "util-extend": "^1.0.1"
  },
  "types": "./index.d.ts",
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".*Test\\.js$"
  }
}
