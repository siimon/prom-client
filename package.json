{
  "name": "prom-client",
  "version": "6.3.0",
  "description": "Client for prometheus",
  "main": "index.js",
  "files": [
    "lib/",
    "index.js",
    "index.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run compile-typescript && npm run test-unit",
    "lint": "node-version-gte-4 && eslint . || node-version-lt-4",
    "test-unit": "mocha --recursive test/",
    "compile-typescript": "tsc index.d.ts --noImplicitAny"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:siimon/prom-client.git"
  },
  "keywords": [
    "Prometheus",
    "Metrics",
    "Client"
  ],
  "author": "Simon Nyberg",
  "license": "Apache-2.0",
  "homepage": "https://github.com/siimon/prom-client",
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^3.5.0",
    "express": "^4.13.3",
    "mocha": "^2.3.4",
    "mockery": "^2.0.0",
    "nock": "^8.0.0",
    "node-version-check": "^2.1.1",
    "sinon": "^1.17.2",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "tdigest": "^0.1.1",
    "util-extend": "^1.0.1"
  },
  "types": "./index.d.ts"
}
